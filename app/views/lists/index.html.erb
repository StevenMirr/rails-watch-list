<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>All Lists</h1>
      <% @lists.each do |list| %>
      <%= link_to list.name, list_path(list) %>
      <% end %>
      <%= link_to 'add List', new_list_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
